{
  "name": "eva",
  "version": "0.0.1",
  "description": "A lisp like script language",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "eva": "dist/cli.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build:parser": "./scripts/build-parser.sh",
